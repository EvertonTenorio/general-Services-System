<div id="<%= dom_id cliente %>">
  <p>
    <strong>Nome:</strong>
    <%= cliente.nome %>
  </p>

  <p>
    <strong>Cpf:</strong>
    <%= cliente.cpf %>
  </p>

  <p>
    <strong>Telefone:</strong>
    <%= cliente.telefone %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= cliente.email %>
  </p>

  <p>
    <strong>Datanascimento:</strong>
    <%= cliente.dataNascimento %>
  </p>

  <p>
    <strong>Endereco:</strong>
    <%= cliente.endereco_id %>
  </p>

</div>
